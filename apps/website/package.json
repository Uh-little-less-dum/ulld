{
    "version": "0.0.1",
    "name": "@ulld/website",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "simulateProduction": "OVERRIDE_PRODUCTION=true next dev",
        "build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 /vercel/path0/node_modules/canvas/build/Release/ && LD_LIBRARY_PATH=/var/task/node_modules/canvas/build/Release next build",
        "buildLocal": "LD_LIBRARY_PATH=/var/task/node_modules/canvas/build/Release dotenv -e .env.local -- next build",
        "start": "next start",
        "generateComponentMap": "tsx src/buildScripts/generateComponentMaps.ts",
        "generateDocs": "tsx --env-file=./.env.local --tsconfig=./tsxConfig.json ./src/buildScripts/generateDocs.ts ",
        "db:migrate": "dotenv -e .env.local -- prisma migrate dev --schema=./src/prisma/schema.prisma",
        "db:generate": "prisma generate --schema=./src/prisma/schema.prisma",
        "migrate:reset": "node ../../node_modules/dotenv-cli/cli.js -e ./website/.env.local -- prisma migrate reset --schema ./src/prisma/schema.prisma",
        "db:seed": "node ../../node_modules/dotenv-cli/cli.js -e ./website/.env.local -- prisma db seed",
        "db:push": "prisma db push --skip-generate",
        "prisma:studio": "dotenv -e .env.local -- prisma studio --schema=./src/prisma/schema.prisma",
        "migrateRemote": "dotenv -e /Users/bigsexy/Desktop/current/ulld/apps/website/.env.remote -- prisma migrate dev --schema=./src/prisma/schema.prisma",
        "postinstall": "turbo db:generate --filter @ulld/website",
        "generateWebTags": "tsx --tsconfig ./tsconfig.json ./scripts/getTagList.ts"
    },
