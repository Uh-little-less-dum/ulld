{
    "name": "@ulld/template",
    "version": "0.0.0",
    "private": true,
    "files": [
        "src"
    ],
    "scripts": {
        "build": "wireit",
        "rebuild": "wireit",
        "db:generate": "wireit",
        "db:migrate": "wireit",
        "db:studio": "dotenv -e .env.local -- prisma studio",
        "dev": "next dev",
        "dev-inspect": "NODE_OPTIONS='--inspect' next dev",
        "dev-inspect:windows": "cross-env NODE_OPTIONS='--inspect' next dev",
        "lint": "wireit",
        "typecheck": "wireit",
        "postinstall": "pnpm run db:generate",
        "predev": "pnpm run db:generate",
        "prepareUpdate": "wireit",
        "runOnBuild": "wireit",
        "start": "next start",
        "toTemplateDeps": "wireit",
        "toWorkspaceDeps": "wireit"
    },
    "wireit": {
        "prepareUpdate": {
            "command": "pnpm syncpack format",
            "clean": false,
            "devOnly": true
        },
        "build": {
            "dependencies": [
                "db:autoMigrate",
                "runOnBuild",
                "clean:preBuild",
                "compile",
                "clean:postBuild"
            ]
        },
        "rebuild": {
            "dependencies": [
                "db:generate",
                "runOnBuild",
                "clean:preBuild",
                "compile",
                "clean:postBuild"
            ]
        },
        "compile": {
            "command": "next build",
            "clean": false
        },
        "db:autoMigrate": {
            "command": "dotenv -e .env.local -- prisma migrate dev --name=ulldAutoMigrate"
        },
        "db:migrate": {
            "command": "dotenv -e .env.local -- prisma migrate dev"
        },
        "db:generate": {
            "command": "dotenv -e .env.local -- prisma generate --schema=./src/database/schema.prisma"
        },
        "runOnBuild": {
            "command": "tsx ./buildUtils/__TEMP__/runOnBuild.ts",
            "clean": false
        },
        "clean:preBuild": {
            "command": "tsx ./__scripts__/build/cleanup/prebuild/main.ts",
            "clean": false
        },
        "clean:postBuild": {
            "command": "tsx ./__scripts__/build/cleanup/postbuild/main.ts",
            "clean": false
        },
        "toWorkspaceDeps": {
            "command": "zsh ./.commitHooks/toWorkspaceDeps.zsh",
            "output": [
                "package.json"
            ],
            "clean": false
        },
        "toTemplateDeps": {
            "command": "tsx ./__scripts__/development/setInternalVersions.ts template",
            "output": [
                "package.json"
            ],
            "clean": false
        },
        "lint": {
            "command": "eslint"
        },
        "typecheck": {
            "command": "tsc --watch --noEmit -p ./tsconfig.json",
            "clean": false,
            "service": true
        }
    },
    "engines": {
        "node": ">=20"
    },
