{
  "name": "@ulld/developer-cli",
  "version": "0.0.0",
  "private": true,
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "ulld-dev": "./bin/run.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "bin": "ulld-dev",
    "dirname": "ulld-dev",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " ",
    "topics": {
      "appendRouter": {
        "description": "Append trpc router while in development"
      }
    }
  },
  "scripts": {
    "dev": "node ./bin/dev.js",
    "build": "rm -rf ./dist && tsc -b",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "lint": "eslint . --ext .ts"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3.2.8",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.24",
    "chai": "^5.1.0",
    "eslint": "^9.0.0",
    "eslint-config-oclif": "^5.1.1",
    "eslint-config-oclif-typescript": "^3.1.4",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10.3.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "@oclif/core": "^3.26.2",
    "@oclif/plugin-autocomplete": "^3.0.13",
    "@oclif/plugin-commands": "^3.2.2",
    "@oclif/plugin-help": "^6.0.20",
    "@oclif/plugin-plugins": "^5.0.7",
    "ts-import": "5.0.0-beta.0"
  },
  "packageManager": "pnpm@9.0.0-beta.1"
}
