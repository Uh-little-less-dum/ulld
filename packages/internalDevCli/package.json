{
  "name": "@ulld/internal-dev-cli",
  "version": "0.0.116",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "mr": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./dist && tsc -b",
    "compile": "oclif pack tarballs",
    "dev": "node ./bin/dev.js",
    "lint": "tsc --watch --noEmit -p ./tsconfigLint.json",
    "test": "jest"
  },
  "types": "dist/index.d.ts",
  "main": "./bin/run.js",
  "exports": "./lib/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@citation-js/plugin-bibtex": "0.7.14",
    "@citation-js/plugin-csl": "0.7.14",
    "@inquirer/prompts": "5.0.1",
    "@oclif/core": "3.26.2",
    "@oclif/plugin-autocomplete": "3.0.13",
    "@oclif/plugin-commands": "3.2.2",
    "@oclif/plugin-help": "6.0.20",
    "@oclif/plugin-plugins": "5.0.7",
    "@oclif/prettier-config": "0.2.1",
    "@oclif/test": "3.2.8",
    "bibtex": "0.9.0",
    "citation-js": "0.7.15",
    "cli-table3": "0.6.4",
    "deepmerge": "4.3.1",
    "glob": "11.0.0",
    "gray-matter": "*",
    "handlebars": "4.7.8",
    "json-schema-to-ts": "3.0.1",
    "marked-terminal": "7.1.0",
    "next": "14.2.3",
    "node-pager": "0.3.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shx": "0.3.4",
    "terminal-link": "3.0.0",
    "ts-import": "5.0.0-beta.0",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "unexpected": "13.2.1",
    "zocker": "1.4.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@inquirer/prompts": "5.0.1",
    "@oclif/core": "3.26.2",
    "@oclif/plugin-autocomplete": "3.0.13",
    "@oclif/plugin-commands": "3.2.2",
    "@oclif/plugin-help": "6.0.20",
    "@oclif/plugin-plugins": "5.0.7",
    "@oclif/prettier-config": "0.2.1",
    "@oclif/test": "3.2.8",
    "@types/chai": "4.3.14",
    "@types/jest": "29.5.11",
    "@types/marked-terminal": "6.1.1",
    "@types/mocha": "10.0.6",
    "@types/node": "20.14.13",
    "bibtex": "0.9.0",
    "chai": "5.1.1",
    "cli-table3": "0.6.4",
    "deepmerge": "4.3.1",
    "eslint": "8.57.1",
    "eslint-config-oclif": "5.1.1",
    "eslint-config-oclif-typescript": "3.1.4",
    "eslint-config-prettier": "9.1.0",
    "glob": "11.0.0",
    "gray-matter": "*",
    "handlebars": "4.7.8",
    "json-schema-to-ts": "3.0.1",
    "mocha": "10.3.0",
    "next": "14.2.3",
    "nyc": "15.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shx": "0.3.4",
    "terminal-link": "3.0.0",
    "ts-import": "5.0.0-beta.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "unexpected": "13.2.1",
    "zocker": "1.4.1"
  },
  "peerDependencies": {
    "@ulld/types": "workspace:*"
  },
  "packageManager": "pnpm@9.0.4",
  "oclif": {
    "bin": "mr",
    "dirname": "mr",
    "commands": {
      "strategy": "pattern",
      "target": "./dist/commands",
      "globPatterns": [
        "**/*.+(js|cjs|mjs|ts|tsx|mts|cts)",
        "!**/*.+(d.*|test.*|spec.*|helpers.*)?(x)"
      ]
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete"
    ],
    "topicSeparator": " "
  }
}
