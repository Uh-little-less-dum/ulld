{
    "version": "0.0.22",
    "name": "@ulld/database",
    "private": false,
    "exports": {
        ".": "./src/db/main.ts",
        "./prisma": "./src/db/main.ts",
        "./internalDatabaseTypes": "./src/db/main.ts",
        "./db": "./src/db/main.ts"
    },
    "files": [
        "src"
    ],
    "scripts": {
        "dev": "echo 'Add dev script here'",
        "build": "echo 'Add build script here'",
        "test": "echo 'Add test script here'",
        "lint": "echo 'Add lint script here'",
        "db:migrateTargetProjectSchema": "dotenv -e /Users/bigsexy/Desktop/current/ulld/.env.local -- prisma migrate dev --schema ./src/prisma/schema.prisma",
        "migrate:reset": "node ../../node_modules/dotenv-cli/cli.js -e .env.local -- prisma migrate reset --schema ./src/prisma/schema.prisma",
        "db:seed": "node ../../node_modules/dotenv-cli/cli.js -e .env.local -- prisma db seed",
        "db:push": "prisma db push --skip-generate",
        "prisma:studio": "node ../../node_modules/dotenv-cli/cli.js -e .env.local -- prisma studio --schema ./src/prisma/schema.prisma",
        "db:generateTargetProjectSchema": "pnpm run generate",
        "generate": "prisma generate",
        "postinstall": "pnpm run generate",
        "prebuild": "pnpm run generate",
        "predev": "pnpm run generate"
