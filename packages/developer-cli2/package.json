{
  "name": "developer-cli2",
  "version": "0.0.0",
  "type": "module",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "prettier --check . && xo && ava"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "ink": "4.4.1",
    "meow": "11.0.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/react": "18.2.79",
    "@vdemedes/prettier-config": "2.0.1",
    "ava": "5.2.0",
    "chalk": "5.3.0",
    "eslint-config-xo-react": "0.27.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "ink-testing-library": "3.0.0",
    "prettier": "3.3.3",
    "ts-node": "10.9.2",
    "typescript": "5.5.4",
    "xo": "0.54.2"
  },
  "prettier": "@vdemedes/prettier-config",
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "license": "MIT"
}
