{
  "name": "@ulld/build",
  "version": "0.0.123",
  "type": "module",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "/bin",
    "/dist/**/*.js",
    "/staticDevelopmentData.json",
    "/oclif.lock",
    "/oclif.manifest.json",
    "/theme.json"
  ],
  "scripts": {
    "build": "pnpm tsc",
    "dev": "echo 'Add dev script here'",
    "lint": "tsc --watch --noEmit -p ./tsconfigLint.json",
    "test": "jest"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc -p . --pretty --incremental",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json"
      ],
      "output": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    }
  },
  "exports": {
    ".": "./dist/index.js",
    "./classes/packageJson": "./dist/classes/pkgJson.js"
  },
  "dependencies": {
    "@antfu/install-pkg": "0.4.1",
    "@commander-js/extra-typings": "12.1.0",
    "@inkjs/ui": "2.0.0",
    "@oclif/core": "3.26.2",
    "@oclif/plugin-autocomplete": "3.0.13",
    "@oclif/plugin-commands": "3.2.2",
    "@oclif/plugin-help": "6.0.20",
    "@oclif/plugin-update": "4.5.5",
    "@oclif/plugin-version": "2.2.11",
    "@oclif/plugin-warn-if-update-available": "3.1.13",
    "ansi-colors": "4.1.3",
    "ansis": "3.3.2",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "cli-spinners": "3.0.0",
    "cli-table3": "0.6.4",
    "cli-welcome": "3.0.0",
    "cli-width": "4.1.0",
    "cliui": "8.0.1",
    "commander": "12.1.0",
    "dedent": "1.5.3",
    "detect-package-manager": "3.0.2",
    "dotenv": "16.4.5",
    "dum": "1.0.0",
    "enquirer": "2.4.1",
    "fs-extra": "11.2.0",
    "glob": "10.3.15",
    "ink": "4.1.0",
    "ink-big-text": "2.0.0",
    "ink-gradient": "3.0.0",
    "ink-link": "4.1.0",
    "ink-markdown": "1.0.4",
    "ink-redux": "1.0.0",
    "ink-responsive-select": "1.3.5",
    "ink-select-input": "6.0.0",
    "ink-spinner": "5.0.0",
    "ink-tab": "5.1.0",
    "ink-table": "3.1.0",
    "ink-text-input": "6.0.0",
    "inquirer": "9.2.23",
    "node-fs-extra": "0.8.2",
    "node-notifier": "10.0.1",
    "nypm": "0.3.11",
    "object-treeify": "4.0.1",
    "ora": "8.0.1",
    "pastel": "2.0.0",
    "pathe": "1.1.2",
    "preferred-pm": "4.0.0",
    "prompts": "2.4.2",
    "react": "18.2.0",
    "shelljs": "0.8.5",
    "simple-git": "3.25.0",
    "terminal-link": "3.0.0",
    "which-pm": "3.0.0",
    "xdg-app-paths": "8.3.0"
  },
  "devDependencies": {
    "ink-testing-library": "4.0.0",
    "@commander-js/extra-typings": "12.1.0",
    "@types/dedent": "0.7.2",
    "@types/fs-extra": "11.0.4",
    "@types/ink-testing-library": "1.0.4",
    "@types/inquirer": "9.0.7",
    "@types/node": "20.14.13",
    "@types/node-notifier": "8.0.5",
    "@types/prompts": "2.4.9",
    "@types/shelljs": "0.8.15",
    "ansi-colors": "4.1.3",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "cli-spinners": "3.0.0",
    "cli-table3": "0.6.4",
    "cliui": "8.0.1",
    "commander": "12.1.0",
    "dotenv": "16.4.5",
    "enquirer": "2.4.1",
    "fs-extra": "11.2.0",
    "inquirer": "9.2.23",
    "node-fs-extra": "0.8.2",
    "ora": "8.0.1",
    "prompts": "2.4.2",
    "simple-git": "3.25.0",
    "terminal-link": "3.0.0",
    "ts-node": "10.9.2",
    "tsx": "*",
    "typescript": "5.5.4"
  },
  "peerDependencies": {
    "@ulld/configschema": "workspace:*",
    "@ulld/developer-schemas": "workspace:*",
    "@ulld/types": "workspace:*",
    "@ulld/utilities": "workspace:*"
  },
  "packageManager": "pnpm@9.0.4",
  "bin": {
    "ulld": "./bin/run.js"
  },
  "oclif": {
    "bin": "ulld",
    "binAliases": [
      "dum"
    ],
    "scope": "ulld",
    "commands": "./dist/commands",
    "theme": "theme.json",
    "state": "beta",
    "dirname": "ulld",
    "topicSeparator": " ",
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command_incomplete.js"
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-version",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    },
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ]
  },
  "repository": "ulld/build"
}
