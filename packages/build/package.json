{
  "name": "@ulld/build",
  "version": "0.0.116",
  "bin": {
    "ulld": "./bin/run.js"
  },
  "dependencies": {
    "@antfu/install-pkg": "^0.4.1",
    "@commander-js/extra-typings": "^12.1.0",
    "@oclif/core": "3.26.2",
    "@oclif/plugin-autocomplete": "3.0.13",
    "@oclif/plugin-commands": "3.2.2",
    "@oclif/plugin-help": "6.0.20",
    "@oclif/plugin-update": "^4.5.5",
    "@oclif/plugin-version": "^2.2.11",
    "@oclif/plugin-warn-if-update-available": "^3.1.13",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@types/prompts": "^2.4.9",
    "ansi-colors": "4.1.3",
    "ansis": "^3.3.2",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "cli-spinners": "3.0.0",
    "cli-table3": "0.6.4",
    "cli-welcome": "^3.0.0",
    "cli-width": "^4.1.0",
    "cliui": "8.0.1",
    "commander": "12.1.0",
    "detect-package-manager": "^3.0.2",
    "dotenv": "16.4.5",
    "dum": "^1.0.0",
    "enquirer": "2.4.1",
    "fs-extra": "^11.2.0",
    "inquirer": "9.2.23",
    "node-fs-extra": "^0.8.2",
    "node-notifier": "^10.0.1",
    "nypm": "^0.3.11",
    "object-treeify": "^4.0.1",
    "ora": "8.0.1",
    "pathe": "^1.1.2",
    "preferred-pm": "^4.0.0",
    "prompts": "2.4.2",
    "shelljs": "^0.8.5",
    "simple-git": "3.25.0",
    "terminal-link": "3.0.0",
    "tsx": "4.15.6",
    "which-pm": "^3.0.0",
    "xdg-app-paths": "^8.3.0"
  },
  "devDependencies": {
    "@commander-js/extra-typings": "^12.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@types/node-notifier": "^8.0.5",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "^0.8.15",
    "ansi-colors": "4.1.3",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "cli-spinners": "3.0.0",
    "cli-table3": "0.6.4",
    "cliui": "8.0.1",
    "commander": "12.1.0",
    "dotenv": "16.4.5",
    "enquirer": "2.4.1",
    "fs-extra": "^11.2.0",
    "inquirer": "9.2.23",
    "node-fs-extra": "^0.8.2",
    "ora": "8.0.1",
    "prompts": "2.4.2",
    "simple-git": "3.25.0",
    "terminal-link": "3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "4.15.6",
    "typescript": "^5.5.2"
  },
  "exports": {
    ".": "./dist/index.js",
    "./classes/packageJson": "./dist/classes/pkgJson.js"
  },
  "files": [
    "/bin",
    "/dist/**/*.js",
    "/staticDevelopmentData.json",
    "/oclif.lock",
    "/oclif.manifest.json",
    "/theme.json"
  ],
  "oclif": {
    "bin": "ulld",
    "binAliases": [
      "dum"
    ],
    "scope": "ulld",
    "commands": "./dist/commands",
    "theme": "theme.json",
    "state": "beta",
    "dirname": "ulld",
    "topicSeparator": " ",
    "flexibleTaxonomy": true,
    "hooks": {
      "command_incomplete": "./dist/hooks/command_incomplete.js"
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-version",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    },
    "additionalVersionFlags": [
      "-v"
    ],
    "additionalHelpFlags": [
      "-h"
    ]
  },
  "packageManager": "pnpm@9.0.4",
  "peerDependencies": {
    "@ulld/configschema": "workspace:*",
    "@ulld/developer-schemas": "workspace:*",
    "@ulld/types": "workspace:*",
    "@ulld/utilities": "workspace:*"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": "ulld/build",
  "scripts": {
    "build": "pnpm tsc",
    "dev": "echo 'Add dev script here'",
    "lint": "tsc --watch --noEmit -p ./tsconfigLint.json",
    "test": "jest"
  },
  "type": "module",
  "wireit": {
    "build": {
      "dependencies": [
        "compile",
        "lint"
      ]
    },
    "compile": {
      "command": "tsc -p . --pretty --incremental",
      "files": [
        "src/**/*.ts",
        "**/tsconfig.json"
      ],
      "output": [
        "dist/**",
        "*.tsbuildinfo"
      ],
      "clean": "if-file-deleted"
    }
  }
}
