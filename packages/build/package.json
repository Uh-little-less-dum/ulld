{
  "name": "@ulld/build",
  "version": "0.0.101",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "bin": {
    "ulld": "./bin/run.js"
  },
  "scripts": {
    "build": "pnpm tsc && tsx ./onBuild.ts",
    "buildTest": "tsup",
    "dev": "echo 'Add dev script here'",
    "lint": "tsc --watch --noEmit -p ./tsconfigLint.json",
    "test": "jest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.24",
    "@types/node-notifier": "^8.0.5",
    "@types/prompts": "^2.4.9",
    "@types/shelljs": "^0.8.15",
    "debug": "^4.3.6",
    "eslint-config-oclif": "^5.1.1",
    "eslint-config-oclif-typescript": "^3.1.4",
    "tsx": "4.15.6",
    "@antfu/install-pkg": "^0.4.1",
    "@commander-js/extra-typings": "^12.1.0",
    "@oclif/core": "3.26.2",
    "@oclif/plugin-autocomplete": "3.0.13",
    "@oclif/plugin-commands": "3.2.2",
    "@oclif/plugin-help": "6.0.20",
    "@oclif/plugin-update": "^4.5.5",
    "@oclif/plugin-version": "^2.2.11",
    "@oclif/plugin-warn-if-update-available": "^3.1.13",
    "ansi-colors": "4.1.3",
    "chalk": "5.3.0",
    "cli-progress": "3.12.0",
    "cli-spinners": "3.0.0",
    "cli-table3": "0.6.4",
    "cliui": "8.0.1",
    "commander": "12.1.0",
    "detect-package-manager": "^3.0.2",
    "dotenv": "16.4.5",
    "dum": "^1.0.0",
    "edit-string": "^1.1.6",
    "enquirer": "2.4.1",
    "fs-extra": "^11.2.0",
    "github-url-to-object": "^4.0.6",
    "inquirer": "9.2.23",
    "node-fs-extra": "^0.8.2",
    "node-notifier": "^10.0.1",
    "nypm": "^0.3.11",
    "object-treeify": "^4.0.1",
    "ora": "8.0.1",
    "preferred-pm": "^4.0.0",
    "prompts": "2.4.2",
    "shelljs": "^0.8.5",
    "simple-git": "3.25.0",
    "terminal-link": "3.0.0",
    "ts-node": "^10.9.2",
    "which-pm": "^3.0.0",
    "xdg-app-paths": "^8.3.0",
    "@ulld/configschema": "workspace:*",
    "@ulld/developer-schemas": "workspace:*",
    "@ulld/types": "workspace:*",
    "@ulld/utilities": "workspace:*"
  },
  "peerDependencies": {},
  "exports": {
    "./classes/packageJson": {
      "require": {
        "default": "./dist/classes/pkgJson.cjs",
        "types": "./dist/classes/pkgJson.d.cts"
      },
      "import": {
        "types": "./dist/classes/pkgJson.d.mts",
        "default": "./dist/classes/pkgJson.mjs"
      }
    },
    "./run/main": {
      "require": {
        "default": "./dist/run/main.cjs",
        "types": "./dist/run/main.d.cts"
      },
      "import": {
        "types": "./dist/run/main.d.mts",
        "default": "./dist/run/main.mjs"
      }
    },
    "./main": {
      "require": {
        "default": "./dist/main.cjs",
        "types": "./dist/main.d.cts"
      },
      "import": {
        "types": "./dist/main.d.mts",
        "default": "./dist/main.mjs"
      }
    }
  },
  "files": [
    "dist",
    "bin",
    "action",
    "staticDevelopmentData.json"
  ],
  "oclif": {
    "bin": "ulld",
    "commands": "./dist/commands",
    "state": "beta",
    "dirname": "ulld",
    "topicSeparator": " ",
    "flexibleTaxonomy": true,
    "npmRegistry": "https://registry.npmjs.org",
    "additionalVersionFlags": [
      "-v",
      "version"
    ],
    "additionalHelpFlags": [
      "-h"
    ],
    "hooks": {
      "command_incomplete": "./dist/hooks/command_incomplete.js"
    },
    "windows": {
      "name": "ULLD cli"
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands",
      "@oclif/plugin-help",
      "@oclif/plugin-update",
      "@oclif/plugin-version",
      "@oclif/plugin-warn-if-update-available"
    ],
    "warn-if-update-available": {
      "timeoutInDays": 7,
      "message": "<%= config.name %> update available from <%= chalk.greenBright(config.version) %> to <%= chalk.greenBright(latest) %>."
    }
  },
  "packageManager": "pnpm@9.0.4",
  "private": false,
  "publishConfig": {
    "access": "public"
  }
}
