{
  "extends": "@ulld/typescript-config/nextjs.json",
  "include": [
    "src**/*.ts",
    "src/**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "dist",
    "build",
    "src/app/**",
    "node_modules",
    ".next"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true,
    "strictNullChecks": true
  }
}
