{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "@ulld/typescript-config/nextjs.json",
  "include": [
    "./**/*.ts",
    "./**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "dist",
    "build",
    "node_modules"
  ],
  "compilerOptions": {
    "rootDir": ".",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "incremental": true
  }
}
