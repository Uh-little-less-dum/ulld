{
    "$schema": "https://json.schemastore.org/tsconfig.json",
      "extends": "@ulld/typescript-config/nextjs.json",
    "include": [
        "./**/*.ts",
        "./**/*.tsx"
    ],
    "exclude": [
        "dist",
        "build",
        "node_modules"
    ],
    "compilerOptions": {
        "rootDir": ".",
        "moduleResolution": "Bundler",
        "jsx": "react-jsx",
        "esModuleInterop": true,
        "resolveJsonModule": true
    }
}

