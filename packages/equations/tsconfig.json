{
  "extends": "@ulld/typescript-config/nextjs.json",
  "compilerOptions": {
    "incremental": true,
    "paths": {
        "@/*": [
            "./*"
        ],
        "#/*": [
            "./src/*"
        ]
    }
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
