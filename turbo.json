{
    "ui": "stream",
  "globalDependencies": [
      "**/.env.*local"
  ],
  "globalEnv": [
      ".env.local",
      "ENABLE_EXPERIMENTAL_COREPACK=1",
      "ULLD_DEV_ROOT",
      "ULLD_CLI_DEVROOT"
  ],
  "tasks": {
    "prepareWeb": {
      "env": [".env.local"]
    },
    "generatePluginConfig": {
      "cache": false,
      "env": [".env.local"]
    },
    "writePluginConfig": {
      "cache": false,
      "env": [".env.local"]
    },
    "generateComponentMap": {
      "cache": false,
      "env": [".env.local"]
    },
    "db:generate": {
      "cache": false,
      "env": [".env.local"]
    },
    "generateTargetDatabase": {
      "cache": false,
      "env": [".env.local"]
    },
    "migrateTargetDatabase": {
      "cache": false,
      "env": [".env.local"]
    },
    "db:push": {
      "cache": false
    },
    "db:migrate": {
      "cache": false,
      "env": [".env.local"]
    },
    "updateJsonSchemas": {
      "env": [".env.production.local", ".env.local", ".env.production", ".env"]
    },
    "build": {
      "dependsOn": ["^db:generate"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [".env.production.local", ".env.local", ".env.production", ".env"]
    },
    "buildLocal": {
      "dependsOn": ["^db:generate", "^prepareWeb", "^generateComponentMap"],
      "outputs": [".next/**", "!.next/cache/**"],
      "env": [".env.production.local", ".env.local", ".env.production", ".env"]
    },
    "test": {
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "test:watch": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^db:generate", "^generateComponentMap"],
      "env": [".env.development.local", ".env.local", ".env"],
      "cache": false,
      "persistent": true
    },
    "dev:debug": {
      "dependsOn": ["^db:generate", "^generateComponentMap"],
      "env": [".env.development.local", ".env.local", ".env"],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
